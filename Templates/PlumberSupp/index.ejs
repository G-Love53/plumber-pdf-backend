<%
/* ========================================================================= */
/* EJS TEMPLATE: BERKLEY CONTRACTOR SUPPLEMENTAL APPLICATION (1:1 MAPPING)    */
/* TARGET PDF: CONTRACTOR SUPP For Form Final.pdf (8 pages)                  */
/* This file must be paired with the custom styles.css file.                 */
/* ========================================================================= */
const segmentName = data.segment || "Contractor";

// Helper function to render X in a checkbox box if the value is 'Yes'
function renderCheckbox(value) {
  return value && value.toLowerCase() === 'yes' ? 'X' : '';
}
%>
<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Contractor Supplemental Application - <%= segmentName %></title>
  <style><%= styles %></style>
</head>
<body>

<div class="header-info">
  <div class="header-row">
    <div class="header-col" style="font-size: 10pt; font-weight: 700;">Berkley Specialty Insurance Company</div>
    <div class="header-col" style="text-align: right;">GF0089 08/18</div>
    <div class="header-col" style="text-align: right;">Email: contactus@berkleyaspire.com</div>
  </div>
  <div class="header-row" style="grid-template-columns: 1fr 1fr;">
    <div class="header-col"><strong>Contractors Supplemental Application</strong></div>
    <div class="header-col" style="text-align: right;"><strong>Applicant:</strong> <%= data.business_name || data.applicant_name || "" %></div>
  </div>
</div>

<h2>Agent & Applicant Information</h2>
<div class="form-section">
  <div class="form-row cols-2">
    <div>
      <div class="form-label">Agent Name:</div>
      <div class="form-value"><%= data.agent_name || "" %></div>
    </div>
    <div>
      <div class="form-label">Agent Number:</div>
      <div class="form-value"><%= data.agent_number || "" %></div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">Contact:</div>
      <div class="form-value"><%= data.applicant_name || "" %></div>
    </div>
    <div>
      <div class="form-label">Phone:</div>
      <div class="form-value"><%= data.business_phone || "" %></div>
    </div>
  </div>
</div>

<h2>Applicant Information</h2>
<div class="form-section">
  <div class="form-row">
    <div class="form-label">Applicant's Name:</div>
    <div class="form-value"><%= data.business_name || data.applicant_name || "" %></div>
  </div>
  <div class="form-row">
    <div class="form-label">Mailing Address:</div>
    <div class="form-value"><%= data.mailing_address || "" %></div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">Phone Number:</div>
      <div class="form-value"><%= data.business_phone || "" %></div>
    </div>
    <div>
      <div class="form-label">Web Address:</div>
      <div class="form-value"><%= data.web_address || "" %></div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">Proposed Policy Period: From</div>
      <div class="form-value"><%= data.policy_period_from || "" %></div>
    </div>
    <div>
      <div class="form-label">To</div>
      <div class="form-value"><%= data.policy_period_to || "" %></div>
    </div>
  </div>
  <div class="form-row">
    <div class="form-label">Business Structure:</div>
    <div class="form-value"><%= data.business_structure || "" %></div>
  </div>
</div>

<h2>Premises Information</h2>
<div class="form-section">
  <div class="form-row">
    <div class="form-label">Location #1</div>
    <div class="form-value"><%= data.mailing_address || data.premise_address || "" %></div>
  </div>
  <div class="form-row">
    <div class="form-label">Location #2</div>
    <div class="form-value"><%= data.location_2 || "" %></div>
  </div>
  <div class="form-row">
    <div class="form-label">Location #3</div>
    <div class="form-value"><%= data.location_3 || "" %></div>
  </div>
</div>

<h2>Underwriting Information</h2>
<div class="form-section">
  <div class="form-row cols-2">
    <div>
      <div class="form-label">1. How long has the applicant been in business? (years)</div>
      <div class="form-value"><%= data.years_in_business || "" %></div>
    </div>
    <div>
      <div class="form-label">Years of experience in this field:</div>
      <div class="form-value"><%= data.years_experience || "" %></div>
    </div>
  </div>
  <div class="form-row cols-3">
    <div>
      <div class="form-label">2. Is the applicant licensed?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.is_licensed) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.is_licensed === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">3. Your contractor's license number:</div>
      <div class="form-value"><%= data.license_number || "" %></div>
    </div>
    <div>
      <div class="form-label">Type of license:</div>
      <div class="form-value"><%= data.license_type || "" %></div>
    </div>
  </div>
  <div class="form-row">
    <div class="form-label">If no, explain (Q2):</div>
    <div class="form-value"><%= data.unlicensed_explanation || "" %></div>
  </div>
  
  <div class="form-row">
    <div class="form-label">4. Indicate the percent of each type of work performed:</div>
    <table class="data-table">
      <thead>
        <tr>
          <th>TYPE</th>
          <th>COMMERCIAL</th>
          <th>RESIDENTIAL</th>
          <th>INDUSTRIAL</th>
          <th>% OF TOTAL OPERATIONS<br>(TOTAL MUST EQUAL 100%)</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><strong>New Construction</strong></td>
          <td><%= data.commercial_new_construction || "" %>%</td>
          <td><%= data.residential_new_construction || "" %>%</td>
          <td><%= data.industrial_new_construction || "" %>%</td>
          <td><%= (data.pct_new_construction || (parseInt(data.commercial_new_construction||0) + parseInt(data.residential_new_construction||0) + parseInt(data.industrial_new_construction||0))) || "" %>%</td>
        </tr>
        <tr>
          <td><strong>Renovation</strong></td>
          <td><%= data.commercial_renovation || "" %>%</td>
          <td><%= data.residential_renovation || "" %>%</td>
          <td><%= data.industrial_renovation || "" %>%</td>
          <td><%= (data.pct_renovation || (parseInt(data.commercial_renovation||0) + parseInt(data.residential_renovation||0) + parseInt(data.industrial_renovation||0))) || "" %>%</td>
        </tr>
        <tr>
          <td><strong>Real Estate Developer</strong></td>
          <td><%= data.commercial_developer || "" %>%</td>
          <td><%= data.residential_developer || "" %>%</td>
          <td><%= data.industrial_developer || "" %>%</td>
          <td><%= (data.pct_developer || (parseInt(data.commercial_developer||0) + parseInt(data.residential_developer||0) + parseInt(data.industrial_developer||0))) || "" %>%</td>
        </tr>
      </tbody>
    </table>
  </div>

  <div class="form-row cols-2">
    <div>
      <div class="form-label">5. (a) General Contractor %</div>
      <div class="form-value"><%= data.pct_gc || "" %>%</div>
    </div>
    <div>
      <div class="form-label">(b) Subcontractor %</div>
      <div class="form-value"><%= data.pct_sub || "" %>%</div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">Indicate the percentage of work on a typical project performed by: (a) Your Employees</div>
      <div class="form-value"><%= data.pct_work_employees || (data.annual_payroll_excl_owners && data.projected_gross_revenue ? ((data.annual_payroll_excl_owners / data.projected_gross_revenue) * 100).toFixed(0) : '') || "" %>%</div>
    </div>
    <div>
      <div class="form-label">(b) Subcontractors under your supervision</div>
      <div class="form-value"><%= data.pct_work_subs || (data.estimated_annual_subcontract_cost && data.projected_gross_revenue ? ((data.estimated_annual_subcontract_cost / data.projected_gross_revenue) * 100).toFixed(0) : '') || "" %>%</div>
    </div>
  </div>
  
  <div class="form-row">
    <div class="form-label">6. If residential construction, how many new detached single family homes per year?</div>
    <div class="form-value"><%= data.single_family_homes_per_year || "" %></div>
  </div>

  <div class="form-row">
    <div class="form-label">7. In the past, have you done any work involving NEW construction of Condominiums, Townhouses, Duplexes, Triplexes, Four-plexes, Patio homes, or single family homes in tract developments?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.past_tract_development) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.past_tract_development === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent cols-2">
    <div>
      <div class="form-label">8. If yes, please specify work completed and maximum number of individual living units in a project:</div>
      <div class="form-value"><%= data.past_tract_details || "" %></div>
    </div>
    <div>
      <div class="form-label">9. When was your most recent project completed?</div>
      <div class="form-value"><%= data.most_recent_project_date || "" %></div>
    </div>
  </div>

  <div class="form-row">
    <div class="form-label">10. Will any of your work during this upcoming policy term involve NEW construction of Condominiums, Townhouses, Duplexes, Triplexes, Four-plexes, Patio homes, or single family homes in tract developments?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.upcoming_tract_development) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.upcoming_tract_development === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, explain and indicate maximum number of units in a project:</div>
    <div class="form-value"><%= data.upcoming_tract_details || "" %></div>
  </div>

  <div class="form-row">
    <div class="form-label">11. Has or will any of your work involve NEW construction of Apartments?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.apartment_work) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.apartment_work === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, explain and indicated maximum number of units in a project:</div>
    <div class="form-value"><%= data.apartment_units || "" %></div>
  </div>
</div>

<div class="page-break"></div>
<div class="header-info">
  <div class="header-row">
    <div class="header-col" style="font-size: 10pt; font-weight: 700;">Berkley Specialty Insurance Company</div>
    <div class="header-col" style="text-align: right;">GF0089 08/18</div>
    <div class="header-col" style="text-align: right;">Email: contactus@berkleyaspire.com</div>
  </div>
  <div class="header-row" style="grid-template-columns: 1fr 1fr;">
    <div class="header-col"><strong>UNDERWRITING INFORMATION (continued)</strong></div>
    <div class="header-col" style="text-align: right;"><strong>Applicant:</strong> <%= data.business_name || data.applicant_name || "" %></div>
  </div>
</div>

<h2>Underwriting Information (continued)</h2>
<div class="form-section">
  <div class="form-row cols-2">
    <div>
      <div class="form-label">12. Do you ever hire subcontractors?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.hire_subs) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.hire_subs === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">13. Are certificates of insurance required from subcontractors prior to the start of every project?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_require_coi) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_require_coi === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">14. Do your subcontractors carry coverage or limits less than yours?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_carry_lower_limits) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_carry_lower_limits === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">If yes, what are the minimum limits you accept?</div>
      <div class="form-value"><%= data.subs_min_limits || "" %></div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">15. Do you require your subcontractors to carry workers compensation?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_wc) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_wc === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">16. Are written contracts, including an indemnification and a hold harmless clause in your favor, obtained from all subcontractors? (A copy of the contract is mandatory to bind coverage)</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_contracts_holdharmless) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_contracts_holdharmless === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">17. Are you named as an additional insured on the subcontractors' policy?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_ai) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_ai === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">18. Does your subcontractor general liability insurance requirements specify that they not have third party action over exclusions?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.subs_no_action_over) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.subs_no_action_over === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>
  <div class="form-row">
    <div class="form-label">19. How long are Certificates of Insurance kept?</div>
    <div class="form-value"><%= data.coi_retention_period || "" %></div>
  </div>

  <div class="form-row">
    <div class="form-label">20. Does your work involve repair or remodeling?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.work_repair_remodel) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.work_repair_remodel === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent cols-2">
    <div>
      <div class="form-label">If yes, have or do you plan to do any remodeling / renovation work for Condominium, Townhome, or Homeowners Associations?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.remodel_condos_hoas) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.remodel_condos_hoas === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">Will any of this work involve condo conversions?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.condo_conversions) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.condo_conversions === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">21. If you have done this work in the past, how long ago? Please describe type of work:</div>
    <div class="form-value"><%= data.past_remodel_work_details || "" %></div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If you plan on doing this type of work during this upcoming policy term, what is the maximum number of individual living units involved in the remodling / renovation project? What type of work will it entail?</div>
    <div class="form-value"><%= data.upcoming_remodel_work_details || "" %></div>
  </div>

  <div class="form-row cols-2">
    <div>
      <div class="form-label">22. Have you ever been involved in or are you aware of pending litigation against you or any named insured concerning construction defect or fungus / mold claims?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.mold_defect_litigation || data.mold_defect_losses) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox((data.mold_defect_litigation === 'No' && data.mold_defect_losses === 'No') ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">23. Are you aware of any loss, damage, or claim that may be made against you or any named insured that involves or alleges construction defect or fungus / mold damage?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.aware_defect_mold) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.aware_defect_mold === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>

  <div class="form-row cols-2">
    <div>
      <div class="form-label">24. Any current Wrap-Up/OCIP Projects?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.wrap_up_projects) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.wrap_up_projects === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
    <div>
      <div class="form-label">If yes, what is the projected cost associated with the Wrap-Up/OCIP Projects?</div>
      <div class="form-value"><%= data.wrap_up_cost || "" %></div>
    </div>
  </div>

  <div class="form-row">
    <div class="form-label">25. Do you have a written safety program?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.written_safety_program) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.written_safety_program === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, describe what safety precautions are in place:</div>
    <div class="form-value"><%= data.safety_precautions || "" %></div>
  </div>
  <div class="form-row">
    <div class="form-label">26. How do you protect the general public from potential injury?</div>
    <div class="form-value"><%= data.protect_public || "" %></div>
  </div>
</div>

<div class="page-break"></div>
<div class="header-info">
  <div class="header-row">
    <div class="header-col" style="font-size: 10pt; font-weight: 700;">Berkley Specialty Insurance Company</div>
    <div class="header-col" style="text-align: right;">GF0089 08/18</div>
    <div class="header-col" style="text-align: right;">Email: contactus@berkleyaspire.com</div>
  </div>
  <div class="header-row" style="grid-template-columns: 1fr 1fr;">
    <div class="header-col"><strong>UNDERWRITING INFORMATION (continued)</strong></div>
    <div class="header-col" style="text-align: right;"><strong>Applicant:</strong> <%= data.business_name || data.applicant_name || "" %></div>
  </div>
</div>

<h2>Underwriting Information (continued)</h2>
<div class="form-section">
  <div class="form-row">
    <div class="form-label">27. Is jobsite security provided at night?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.jobsite_security_night) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.jobsite_security_night === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, please describe:</div>
    <div class="form-value"><%= data.jobsite_security_details || "" %></div>
  </div>
  <div class="form-row cols-2">
    <div>
      <div class="form-label">28. What is the maximum height of buildings you work on? (# stories)</div>
      <div class="form-value"><%= data.max_building_height || "" %></div>
    </div>
    <div>
      <div class="form-label">29. Does a foreman or qualified individual inspect all jobs upon completion?</div>
      <div class="yesno-line">
          <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.completion_inspection) %></span></span>
          <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.completion_inspection === 'No' ? 'Yes' : '') %></span></span>
      </div>
    </div>
  </div>

  <div class="form-row">
    <div class="form-label">30. Do you perform any out of state work?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.out_of_state_work) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.out_of_state_work === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, in what states? (provide details of work performed)</div>
    <div class="form-value"><%= data.out_of_state_details || data.states_work_in || "" %></div>
  </div>
  <div class="form-row">
    <div class="form-label">31. Have you, will you ever, or do you currently perform work in AZ, CA, CO, NV, NY, OR, UT or WA?</div>
    <div class="yesno-line">
        <span class="checkbox-inline">Yes <span class="checkbox-box"><%= renderCheckbox(data.work_restricted_states) %></span></span>
        <span class="checkbox-inline">No <span class="checkbox-box"><%= renderCheckbox(data.work_restricted_states === 'No' ? 'Yes' : '') %></span></span>
    </div>
  </div>
  <div class="form-row indent">
    <div class="form-label">If yes, please provide details of work performed:</div>
    <div class="form-value"><%= data.restricted_states_details || "" %></div>
  </div>

  <div class="form-row">
